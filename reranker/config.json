{
    "model_name": "gte-multilingual-reranker-base",
    "cache_dir": "/reranker/models",
    "max_length": 512,
    "batch_size": {
        "cpu": 16,
        "gpu": 512
    },
    "profile_enabled": false,
    "memory_optimize": {
        "use_fp16": true,
        "empty_cache_interval": 3,
        "reserve_memory": true
    },
    "perf_logging": {
        "detailed_timings": true,
        "log_gpu_memory": true,
        "log_intervals": [50, 100, 500]
    }
} 